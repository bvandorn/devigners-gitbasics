<div class="centered">
  <h1>Create New Puppy</h1>
  <%= form_for @puppy do |f| %>
    <table>
      <tr><td><%= f.label :name %></td><td><%= f.text_field :name %></td></tr>
      <tr><td><%= f.label :image %></td><td><%= f.text_field :image %></td></tr>
      <tr><td><%= f.label :age %></td><td><%= f.range_field :age %></td></tr>
      <tr><td></td><td><%= f.submit %></td></tr>
    </table>
  <% end %>
</div>
